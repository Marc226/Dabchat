  <property name="module.jdk.home.loginservice" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.loginservice" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.loginservice" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.loginservice" value="-encoding UTF-8 -source 8 -target 1.8 ${compiler.args}"/>
  
  <property name="loginservice.output.dir" value="${module.loginservice.basedir}/target/classes"/>
  <property name="loginservice.testoutput.dir" value="${module.loginservice.basedir}/target/test-classes"/>
  
